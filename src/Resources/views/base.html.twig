    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1" name="viewport" />
    <!-- BEGIN GLOBAL MANDATORY STYLES -->
    {% block stylesheets %}

        {% stylesheets
        "@randock_metronic_css"
        "@randock_metronic_scss"
            
